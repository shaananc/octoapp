 <%= semantic_form_for @hookup do |f| %>
    <%= f.semantic_errors %>
    <%= f.inputs do %>
    <%= f.input :person, :name => "person", :as => :select, :collection => Person.all, :label => "Person" %>
    <%= f.input :date %>
    <%= f.input :partnera %>
    <%= f.input :partnerb %>
    <% end %>
    <%= f.actions :submit, :cancel%>

  <% end %>